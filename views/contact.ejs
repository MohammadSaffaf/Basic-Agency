    <header class="contacts-header">
        <div class="contact-img">

        </div>
        <div class="contact-info">
            <h1>B®/</h1>
            <h1>CONTACT</h1>
            <p class="easy-text">EASY TO UNDERSTAND. IMPOSSIBLE TO IGNORE.™ BASIC®, INC 10 - 20©
            </p>
            <div class="contact-mail">
                <h2>●</h2>
                <div>
                    <h3>NEW BUSINESS</h3>
                    <h3>GENERAL</h3>
                    <p>hi@mail.com</p>

                    <p>hi@mail.com</p>
                    <h3>PRESS</h3>
                    <h3>JOIN US</h3>
                    <p>hi@mail.com</p>

                    <p>hi@mail.com</p>
                </div>
            </div>
        </div>
    </header>
    <section class="contact-office">
        <article class="contact-SEC">
            <p>SEC.</p>
            <h1>OFFICES</h1>
        </article>
        <article class="article-office">
            <div class="div-office">
                <p>/A</p>
                <h3>●</h3>

            </div>

            <div class="sandiego">
                <% office.forEach(officeItem=> { %>
                    <div class="bayarfa">
                        <figure class="photo-office">
                            <img src="<%= officeItem.img %> " alt="">
                            <figcaption>
                                <h4>
                                    <%= officeItem.number %>
                                </h4>
                                <h4>
                                    <%= officeItem.secNumber %>
                                </h4>

                            </figcaption>

                        </figure>
                        <h3>
                            <%= officeItem.place %>
                        </h3>
                    </div>


                    <%})%>
            </div>
        </article>
    </section>
    <section class="contact-office">
        <article class="contact-SEC">
            <p>SEC.</p>
            <h1>FOLLOW</h1>
        </article>
        <article class="article-office">
            <div class="div-office">
                <p>/B</p>
                <h3>●</h3>

            </div>
            <div class="contact-social">
                <div class="div-social">
                    <h2>Social</h2>
                    <ul>

                        <li><a href="#">Instagram</a></li>
                        <li><a href="#">Twitter</a></li>
                        <li><a href="#">Linkedin</a></li>
                        <li><a href="#">Facebook</a></li>
                    </ul>
                </div>
                <div class="contact-input">
                    <h5>STAY IN THE KNOW</h5>
                    <p>Email Address</p>
                    <input type="email" name="email">
                </div>
            </div>


        </article>
    </section>
